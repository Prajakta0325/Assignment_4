   "source": [
    "# Write a Python program to square the elements of a list using map() function."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "636a6820",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "4 5 2 9\n",
      "Sample List :  [4, 5, 2, 9]\n",
      "Square of Elements: [16, 25, 4, 81]\n"
     ]
    }
   ],
   "source": [
    "l=list(map(int,input().split()))\n",
    "print(\"Sample List : \",l)\n",
    "\n",
    "result= list(map(lambda x:x**2,l))\n",
    "print(\"Square of Elements:\",result)"
   ]
